-- (x,y) to r
c2pr = [(float,float) -> float] <<<
  $out = sqrt($in1 * $in1 + $in2 * $in2);
>>>

-- (x,y) to theta
c2pt = [(float,float) -> float] <<<
  $out = atan2($in2,$in1);
>>>

-- Polar to Cartesian

-- (r,theta) -> x
p2cx = [(float,float) -> float] <<<
  $out = $in1 * cos($in2);
>>>

-- (r,theta) -> y
p2cy = [(float,float) -> float] <<<
  $out = $in1 * sin($in2);
>>>

-- (r,theta) -> (x,y)
p2c = &1(1,1);{p2cx,p2cy}

test1 = &1(1,1)
test2 = {p2cx,p2cy}
