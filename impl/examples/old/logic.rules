not1 = [ not(true) -> false ]
not2 = [ not(false) -> true ]

or1 = [ or(P,true) -> true ]
or2 = [ or(true,P) -> true ]
or3 = [ or(P,false) -> P ]
or4 = [ or(false,P) -> P ]
or5 = [ or(P,not(P)) -> true ]
or6 = [ or(not(P),P) -> true ]

and1 = [ and(P,false) -> false ]
and2 = [ and(false,P) -> false ]
and3 = [ and(P,true) -> P ]
and4 = [ and(true,P) -> P ]
and5 = [ and(P,not(P)) -> false ]
and6 = [ and(not(P),P) -> false ]

reduceConstsNot = not1 | not2
reduceConstsOr = or1 | or2 | or3 | or4
reduceConstsAnd = and1 | and2 | and3 | and4
reduceConsts = reduceConstsNot | reduceConstsAnd | reduceConstsOr

reduceTruism = and5 | and6 | or5 | or6

reduce = reduceConsts | reduceTruism
