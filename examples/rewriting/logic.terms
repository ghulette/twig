-- The following terms are of the form or(P,not(P)) and and(not(P),P).
-- Notice that we rely on the rewriting system ensuring that the bindings to
-- P take on the same term.

reduce or(or(and(p,q),r),not(or(and(p,q),r)))

reduce and(not(or(and(p,q),r)),or(and(p,q),r))

-- This one should not match, because p != q
reduce or(p,not(q)) 
