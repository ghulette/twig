dblNeg = [ not(not(P)) -> P ]

demorgan1 = [ not(or(P,Q)) -> and(not(P),not(Q)) ]
demorgan2 = [ not(and(P,Q)) -> or(not(P),not(Q)) ]

demorgan = demorgan1 | demorgan2

dist1 = [ and(P,or(Q,R)) -> or(and(P,Q),and(P,R)) ]
dist2 = [ and(or(P,Q),R) -> or(and(P,R),and(Q,R)) ]
dist3 = [ or(P,and(Q,R)) -> and(or(P,Q),or(P,R)) ]
dist4 = [ or(and(P,Q),R) -> and(or(P,R),or(Q,R)) ]

try(s) = s | T
repeat(s) = try(s;repeat(s))
oncetd(s) = s | One oncetd(s)

cnf = repeat(oncetd(dblNeg | demorgan | dist3 | dist4))
